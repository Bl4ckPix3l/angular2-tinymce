    "core-js": "^2.4.1",
    "rxjs": "^5.0.3",
    "tinymce": "^4.5.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.10",
    "@angular/compiler-cli": "^4.0.0",
